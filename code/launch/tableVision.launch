<launch>

<!--

    <param name="sideKinect/driver/depth_time_offset" value="-0.13" />

    <include file="$(find openni_launch)/launch/openni.launch">
    <arg name="device_id" value="B00366212349051B"/>
    <arg name="camera" value="sideKinect" />
    <arg name="rgb_camera_info_url" value="package://objRec_comm/camera_calibration/rgb_sideKinect.yaml" />
    <arg name="depth_camera_info_url" value="package://objRec_comm/camera_calibration/depth_sideKinect.yaml" />
